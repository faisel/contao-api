{
  "name": "faisel/contao-api",
  "description": "Access Contao content via an easy to use JSON-API",
  "type": "contao-bundle",
  "keywords": [
    "contao",
    "api",
    "bundle",
    "json"
  ],
  "require": {
    "php": "^5.6|^7.0",
    "contao/core-bundle": "^4.4"
  },
  "require-dev": {
    "evert/phpdoc-md": "~0.2.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Faisel Kachappilly",
      "email": "faisel.kachappilly@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DieSchittigs\\ContaoContentApiBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "DieSchittigs\\ContaoContentApiBundle\\ContaoManager\\Plugin"
  }
}
